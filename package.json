{
  "name": "taste-of-artisan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run imagemin && next build",
    "start": "next start",
    "deploy": "now",
    "storybook": "start-storybook -s ./public -p 6006 --quiet",
    "build-storybook": "build-storybook -s ./public",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "tsc:watch": "tsc --watch",
    "imagemin": "node optimize-images.js",
    "forestry:preview": "npm run imagemin && next -p 8080 -H 0.0.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "date-fns": "^2.11.1",
    "framer-motion": "^1.10.2",
    "lodash": "^4.17.15",
    "next": "^9.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-use": "^13.27.1",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.27",
    "@types/showdown": "^1.9.3",
    "@zeit/next-css": "^1.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "husky": "^4.2.3",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "raw-loader": "^4.0.0",
    "tailwindcss": "^1.1.4",
    "typescript": "^3.8.3"
  }
}
